   "outputs": [],
   "source": [
    "i = 0\n",
    "for y in 1:length(words)\n",
    "    x = (3*(y - 1) + 1) % length(words[1])\n",
    "    if x == 0\n",
    "        x = 31\n",
    "    end\n",
    "    if words[y][x] == \"#\"[1]\n",
    "        i += 1\n",
    "    end\n",
    "end"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(i)"
   ]
  }
